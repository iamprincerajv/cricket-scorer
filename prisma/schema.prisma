generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Player {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  userId String
  image String?
  name String?
  desc String?
  isCaptain Boolean? @default(false)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  @@map("players")
}